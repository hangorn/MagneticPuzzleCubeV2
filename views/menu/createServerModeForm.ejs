<div class="msgMenu">
	<h1><%= __('menu.createServer') %></h1>
	<p><%= __('dialogs.multiplayer.chooseKindOfGame') %> :</p>
	<div class="menuContainer leftContainer">
		<% for (var i=0; i < levelsData.length; i++) { %>
		<div class="clickable menuItem blockMenuItem">
			<img name="img" alt="<%= levelsData[i].description %>" title="<%= levelsData[i].description %>"
				src="<%= levelsData[i].img %>">
			<p><%= levelsData[i].name %></p>
		</div>
		<% } %>
	</div>
	<div class="rightContainer">
		<label for="gameName"><%= __('dialogs.multiplayer.enterGameName') %>:</label>
		<input id="gameName" type="text" maxlength="20" name="gameName">
		<p class="description"></p>
	</div>
</div>